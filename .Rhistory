library(Lahman)
library(sqldf)
query<-"SELECT yearID, HR
FROM Batting
WHERE playerID='ruthba01'"
result<-sqldf(query)
ggplot()+
geom_line(data=result,aes(x=yearID,y=HR))
ggplot()+
geom_line(data=result,aes(x=yearID,y=HR))+
ggtitle("Ruth's Homerun Totals Through the Years")+
xlab("year")+
ylab("homerun")
ggplot()+
geom_line(data=result,aes(x=yearID,y=HR),color=green)+
ggtitle("Ruth's Homerun Totals Through the Years")+
xlab("year")+
ylab("homerun")
ggplot()+
geom_line(data=result,aes(x=yearID,y=HR))+
geom_point(data=result,aes(x=yearID,y=HR))+
ggtitle("Ruth's Homerun Totals Through the Years")+
xlab("year")+
ylab("homerun")
